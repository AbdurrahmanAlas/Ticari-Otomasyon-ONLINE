@using ONLINETICARIOTOMASYON.Models.Sınıflarım
@model List<FaturaKalem>
@{
    ViewBag.Title = "FaturaDetay";
    Layout = "~/Views/Shared/AdmınLayout.cshtml";
}

<br />


<h2>Fatura Kalemleri sayfası</h2>

<br />


<ol class="breadcrumb mb-4">


    <li class="breadcrumb-item active"></li>

</ol>

<table class="table table-hover">

    <tr>

        <th> ID</th>
        <th>AÇIKLAMA</th>
        <th>MİKTAR</th>
        <th>BİRİM FİYAT</th>
        <th>TUTAR</th>



    </tr>

    @foreach (var x in Model)
    {
        <tr>
            <td>@x.FaturaKalemID</td>
            <td>@x.Acıklama</td>
            <td>@x.Mıktar</td>
            <td>@x.BırımFıyat</td>
            <td>@x.Tutar</td>
         





        </tr>


    }


</table>

<a href=" /Fatura/YenıKalem" class="btn btn-default" style="background-color:aquamarine">YENİ KALEM GİRİŞİ</a>