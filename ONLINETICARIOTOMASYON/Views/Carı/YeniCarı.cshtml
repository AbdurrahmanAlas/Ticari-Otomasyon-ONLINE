@model ONLINETICARIOTOMASYON.Models.Sınıflarım.Carıler

@{
    ViewBag.Title = "YeniCarı";
    Layout = "~/Views/Shared/AdmınLayout.cshtml";
}

<h2>Yeni cari sayfası</h2>

<br />
<form class="form-group" method="post">
    @Html.LabelFor(x => x.CarıAd)
    @Html.TextBoxFor(x => x.CarıAd, new { @class = "form-control" })

    <br />
    @Html.LabelFor(x => x.CarıSoyad)
    @Html.TextBoxFor(x => x.CarıSoyad, new { @class = "form-control" })

    <br />
    @Html.LabelFor(x => x.CarıSehır)
    @Html.TextBoxFor(x => x.CarıSehır, new { @class = "form-control" })

    <br />
    @Html.LabelFor(x => x.CarıMaıl)
    @Html.TextBoxFor(x => x.CarıMaıl, new { @class = "form-control" })

    <br />
  

    <button class="btn btn-info">KAYDET</button>


</form>




